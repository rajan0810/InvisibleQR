Entity: HiddenMessage
Attributes:
- id: UUID (required)
- fingerprint: String (required) - The texture fingerprint
- encryptedContent: Data (required) - Encrypted message
- timestamp: Date (required)
- locationHint: String (optional)
- isRevealed: Boolean (default: false)

Entity: TextureFingerprint
Attributes:
- id: UUID (required)
- fingerprintHash: String (required)
- confidence: Double (required)
- creationDate: Date (required)

